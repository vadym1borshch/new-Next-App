{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "import/first": "error",
    "import/newline-after-import": "error",
    "import/no-duplicates": "error",
    "no-console": [
      "warn",
      {
        "allow": [
          "error"
        ]
      }
    ]
  }
}
